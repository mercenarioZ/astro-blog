---
import Body from "../components/Body.astro";
import MainHead from "./MainHead.astro";

interface Props {
  title: string;
  description?: string;
}

const { title, description } = Astro.props as Props;
---

<!doctype html>
<html lang="en">
  <MainHead title={title} description={description} />
  <Body>
    <div>
      <slot />
    </div>
  </Body>
</html>

