---
import Body from "../components/Body.astro";
import MainHead from "./MainHead.astro";

interface Props {
  title: string;
  description?: string;
}

const { title } = Astro.props as Props;
---

<!doctype html>
<html lang="en">
  <MainHead title={title} />
  <Body>
    <slot />
  </Body>
</html>
